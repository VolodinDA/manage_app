<% provide(:title, @errand.address) %>
<h1><%= @errand.address %></h1>
<div class="row">

  <div class="col-lg-7">
    <div class="panel panel-info">
      <div class="panel panel-heading">
        <h3 class="panel-title">Stuff</h3>
      </div>
      <% if !@users.nil? %>
        <ul class="list-group">
          <% @users.each do |user| %>
            <li class="list-group-item">
              <h4><%= link_to user.name, user %></h4>
            </li>
          <% end %>
        </ul>
      <% end %>
    </div>
  </div>

</div>